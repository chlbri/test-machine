{
  "version": "0.1.731",
  "license": "MIT",
  "name": "@core_chlbri/test-machine",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "pnpm run test && pnpm build:-test",
    "build:-test": "pnpm run tsc && pnpm run lint",
    "test": "jest",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx,yml,json}\"",
    "lint": "pnpm prettier && pnpm eslint -- --fix",
    "rinit": "rimraf -rf node_modules && pnpm install",
    "npm:prepublish": "ts-node \"./wconfig/pack.ts\"",
    "npm:publish": "git checkout main && pnpm npm:prepublish && pnpm publish --access public --no-git-checks && ts-node \"./wconfig/clean.ts\"",
    "publish:all": "pnpm git-publish && pnpm npm:publish",
    "upgrade:core": "pnpm upgrade @core_chlbri/core",
    "upgrade": "pnpm upgrade",
    "git-push": "git push --follow-tags",
    "git-publish": "pnpm run rinit && pnpm build && publish"
  },
  "author": "chlbri",
  "dependencies": {
    "@core_chlbri/core": "^0.1.1-dc",
    "jest": "^27.3.1",
    "nanoid": "^3.1.23",
    "ts-jest": "^27.0.7",
    "xstate": "^4.25.0"
  },
  "devDependencies": {
    "@types/edit-json-file": "^1.6.1",
    "@types/jest": "^27.0.3",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "edit-json-file": "^1.6.2",
    "eslint": "7.32.0",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "typescript": "^4.5.2"
  }
}
