{
  "version": "0.1.3",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "publish": "np --branch beme-"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.25.0",
    "**/@typescript-eslint/parser": "^4.25.0",
    "**/typescript": "^4.2.4"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "test-machine",
  "author": "chlbri",
  "module": "dist/test-machine.esm.js",
  "size-limit": [
    {
      "path": "dist/test-machine.cjs.production.min.js",
      "limit": "50 KB"
    },
    {
      "path": "dist/test-machine.esm.js",
      "limit": "50 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.11.0",
    "husky": "^6.0.0",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "nanoid": "^3.1.23",
    "xstate": "^4.19.1"
  }
}
